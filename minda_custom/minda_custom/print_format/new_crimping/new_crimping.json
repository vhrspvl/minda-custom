center>\n</td>\n{% elif row.legend == 'L2-Can Work under Supervision' %}\n<td style=\"border:1px solid !important;\"><center>\n<table border=\"1\"><tr><td style=\"border:1px solid !important;background-color:#22b14c !important\"></td><td style=\"border:1px solid !important;background-color:#22b14c !important\"></td></tr>\n<tr><td style=\"border:1px solid !important;\"></td><td style=\"border:1px solid !important;\"></td></tr>\n</table></center>\n</td>\n{% elif row.legend == 'L3-Can Work Independently' %}\n<td style=\"border:1px solid !important;\"><center>\n<table border=\"1\"><tr><td style=\"border:1px solid !important;background-color:#22b14c !important\"></td><td style=\"border:1px solid !important;background-color:#22b14c !important\"></td></tr><tr><td style=\"border:1px solid !important;background-color:#22b14c !important\"></td><td style=\"border:1px solid !important;\"></td></tr>\n</table></center>\n</td>\n{% elif row.legend == 'L4-Can Train Others' %}\n<td style=\"border:1px solid !important;\"><center>\n<table border=\"1\"><tr><td style=\"border:1px solid !important;background-color:#22b14c !important\"></td><td style=\"border:1px solid !important;background-color:#22b14c !important\"></td></tr><tr><td style=\"border:1px solid !important;background-color:#22b14c !important\"></td><td style=\"border:1px solid !important;background-color:#22b14c !important\"></td></tr>\n</table></center>\n</td>\n{% else %}\n<td style=\"border:1px solid !important;\"></td>\n{% endif %}\n</tr>\n{%- endfor -%}\n</table><br><br><br>\n{{ footer }}\n</html>", 
